# .env.example

# Enode OAuth and API credentials (replace with real credentials in .env)
ENODE_CLIENT_ID=your_enode_client_id
ENODE_CLIENT_SECRET=your_enode_client_secret

# Base API URLs (use sandbox for development)
ENODE_BASE_URL=https://enode-api.sandbox.enode.io
ENODE_AUTH_URL=https://oauth.sandbox.enode.io/oauth2/token

# Redirect URI used for linking (local dev)
REDIRECT_URI=http://localhost:8000/api/link/callback
 